{
  "name": "umami-api-docu",
  "version": "1.0.0",
  "dependencies": {
    "@redocly/cli": "1.34.5"
  },
  "private": true,
  "scripts": {
    "check": "redocly check-config",
    "stats": "redocly stats openapi/openapi.yaml",
    "start": "redocly preview-docs",
    "build": "mkdir -p dist && redocly build-docs openapi/openapi.yaml --output=dist/index.html --template=docs/index.html",
    "test": "redocly lint",
    "arazzo:build": "redocly generate-arazzo openapi/openapi.yaml -o arazzo.test.gen.yaml",
    "arazzo:test": "redocly respect arazzo.test.yaml"
  }
}
